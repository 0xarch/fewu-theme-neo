include /mixins/script.pug
doctype html
html 
    head 
        +meta(user.note,varias.name)
        +BasicStyleSheet
        +BasicECMAScript
        +use('KPageNavigator')
        title= __title__(varias.name,user.name)
    body(OCP='OVERRIDE')
        +NavBar
        +PageLabel(i18n('Category')+' - '+varias.name)
        main
            ul.cPostCon.uWcon
                each bid in cycling.value
                    +SinglePost(IDMap.get(+bid))
                +KPageNavigator(cycling.total,cycling.now,'category/s'+varias.name)
        +Footer